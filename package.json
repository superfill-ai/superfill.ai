{
  "name": "superfill.ai",
  "description": "AI-powered form filling browser extension",
  "private": true,
  "version": "0.2.3",
  "type": "module",
  "author": {
    "email": "mihirpandey.13@gmail.com",
    "name": "Mihir Kumar"
  },
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "dev:safari": "wxt -b safari",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:safari": "wxt build -b safari",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:safari": "wxt zip -b safari",
    "type-check": "tsgo --noEmit",
    "postinstall": "wxt prepare && bun run copy-pdfjs-worker",
    "prepare": "simple-git-hooks",
    "copy-pdfjs-worker": "node scripts/copy-pdfjs-worker.js",
    "format": "biome check --write",
    "lint": "bun run format",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:status": "changeset status",
    "version": "changeset version && bun run sync-version",
    "sync-version": "node scripts/sync-version.js",
    "release": "bun run build && bun run zip && changeset publish"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.57",
    "@ai-sdk/google": "^2.0.52",
    "@ai-sdk/openai": "^2.0.89",
    "@langfuse/otel": "^4.5.1",
    "@langfuse/tracing": "^4.5.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-web": "^2.4.0",
    "@radix-ui/react-accordion": "^1.2.12",
