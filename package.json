{
  "name": "@superfill/monorepo",
  "private": true,
  "version": "0.2.2",
  "description": "Superfill.ai monorepo - AI-powered form filling for browser and desktop",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "simple-git-hooks",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:status": "changeset status",
    "version": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "simple-git-hooks": "^2.13.1",
    "turbo": "^2.3.5",
    "typescript": "^5.9.3"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run lint && bun run type-check"
  },
  "packageManager": "bun@1.3.5",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "catalogs": {
    "default": {
      "@ai-sdk/anthropic": "^2.0.57",
      "@ai-sdk/google": "^2.0.52",
      "@ai-sdk/openai": "^2.0.89",
      "@langfuse/otel": "^4.5.1",
      "@langfuse/tracing": "^4.5.1",
      "@opentelemetry/api": "^1.9.0",
      "@opentelemetry/sdk-trace-web": "^2.4.0",
      "@tanstack/react-query": "^5.90.19",
      "ai": "^5.0.121",
      "ai-sdk-ollama": "^2.2.0",
      "lucide-react": "^0.561.0",
      "sonner": "^2.0.7",
      "zod": "^4.3.5"
    }
  }
}
